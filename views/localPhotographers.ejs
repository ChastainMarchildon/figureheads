<% include partials/header %>

<div class="container" style="overflow:scroll">
        <div class="hero hero-sm">
          <div class="col-10 col">
            <h1 style="float:left">
              <%= title %>
              <% include partials/photographerFilter %>
            </h1>
          </div>
        </div>
      
        <br>
        <table class="table table-striped">
          <thead>
            <tr>
              <!--<th></th>-->
              <th>Name</th>
              <th>Location</th>
              <th>Portfolio</th>
              <th>Instagram</th>
            </tr>
          </thead>
          <tbody>
      
            <% for (let i = 0; i < users.length; i++) { %>
      
              <tr>
                <!-- pics broken atm
                <td>
                    <img src="/images/<%= users[i]._id %>.png" style="max-height:100px; max-width:100px;" />
                </td>
                -->
                <td>
                    <%= users[i].name %>
                </td>
                <td>
                  <%= users[i].location %>
              </td>
                <td>
                  <a href="<%= users[i].portfolioLink %>"><%= users[i].portfolioLink %></a>
                </td>
                <td>
                    <a href="<%= users[i].instagram %>"><%= users[i].instagram %></a>
                  </td>
      
              <% } %>
          </tbody>
        </table>
      </div>
<% include partials/footer %>